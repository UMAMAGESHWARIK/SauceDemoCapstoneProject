<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="SauceDemo Suite" parallel="false">
    <listeners>
        <listener class-name="utils.TestListener"/>
    </listeners>
    <test name="All Tests">
        <classes>
            <!-- Cucumber Runner -->
            <class name="runners.TestRunner"/>

            <!-- Your TestNG Classes -->
            <class name="testcases.ProductsTest"/>
